package com.koreait.first.loop;

public class For {

	public static void main(String[] args) {
		
		int j=0;
		
		for(int i=0; i<10; i++) {
			System.out.println("Hi~ " + i);
		}
		
		System.out.println("-----------------");
		
		for(int i=50; i<101; i++) {
			j++;
			System.out.println(i);
		}
		
		System.out.println(j);
		
	}
	
}
