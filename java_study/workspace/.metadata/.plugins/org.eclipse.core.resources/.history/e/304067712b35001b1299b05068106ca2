package com.koreait.first.obj2;

public class Animal {
	String name;
	int age;
	
	public Animal() {						//기본생성자 작성하지 않아도 컴파일러가 자동으로 넣어준다.
		super();									//super()는 생략가능하다.
		System.out.println("-- Aniaml 기본 생성자() --");
	}
	
	public Animal(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public void crying() {
		System.out.printf("%d살의 %s가 운다.\n",age,name);
	}

}
