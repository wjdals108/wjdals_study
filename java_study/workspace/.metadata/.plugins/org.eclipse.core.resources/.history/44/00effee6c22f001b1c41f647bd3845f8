package com.koreait.first.loop;

public class For6 {

	public static void main(String[] args) {
		int star = 5;
		
		String result = " ";
		
		for(int i=0; i<star; i++) {
			result += "*";
			result += "\n";
			for(int j=0; j<star; j++) {
				result += "*";
			}
			
		}
		System.out.println(result);
		
		
	}
}