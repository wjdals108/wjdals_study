package com.koreait.first.array;

public class Array4 {

	public static void main(String[] args) {
		int[] arr = {7, 4, 5, 1, 3};
		
		int n;
		for(int i=0; i<arr.length; i++) {
			if(arr[i]>arr[i+1]) {
				n=arr[i+1];
				arr[i]=arr[i+1];
				arr[i+1]=n;
			}
			
		}
		
		
		
		
		
		
		for(int i=0; i<arr.length; i++) {
			System.out.print(arr[i]+",");
		}
				
	}
}