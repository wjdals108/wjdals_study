package com.koreait.first.obj2;

public class ClassA {
	private String name;
	
	public ClassA() {
		System.out.println("ClassA 생성자");
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}

class ClassB extends ClassA{
	private int age;

	public ClassB() {
		System.out.println("ClassB 생성자");
	}
	
	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
}

class ClassC extends ClassB{
	
	
	public ClassC() {
		System.out.println("ClassC 생성자");
	}
	
	public ClassC(String name, int age) {
		setName(name);				//this.name 은 name이 private형태이기때문에 못쓴다.
		setAge(age);
	}
	
	
	public void print() {
		System.out.printf("저는 %s이고, 나이는 %d살 입니다.\n", getName(), getAge());
	}	
}

class ClassD extends ClassC{
	public ClassD() {
		
		super("a",3);
		System.out.println("ClassD 생성자");
	}
	
}