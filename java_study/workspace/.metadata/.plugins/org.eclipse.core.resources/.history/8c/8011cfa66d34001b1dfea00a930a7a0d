package com.koreait.first.obj;

public class ObjTest5  {

	public static void main(String[] args) {						//파일명과 똑같은 이름의 클래스만 앞에 public을 붙일 수 있다. (안붙여도됨)
		NumberBox2 nbb2 = new NumberBox2();
		nbb2.a=10;
		nbb2.b=20;
		
		NumberBox2 nbb22 = new NumberBox2();
		nbb22.a = 33;
		nbb22.b = 44;
		
		System.out.println("nbb2 : " + nbb2.sum());
		System.out.println("nbb22 : " + nbb22.sum());
	}
}

class NumberBox2{
	int a;
	int b;
	
	int sum() {
		return a+b;
	}
}