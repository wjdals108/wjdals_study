package com.koreait.first.loop;

public class For3 {

	public static void main(String[] args) {
		
		for(int i=1; i<24; i++) {
			
			for(int z=0; z<=59; z++) {
				
				System.out.printf("%d : %d \n", i, z);
			}
			System.out.println("ë•¡");
		}
		
		
		for(int dan=2; dan<10; dan++) {
			
			for(int a=1; a<10< a++) {
				
			}
			
		}
				
	}
}
