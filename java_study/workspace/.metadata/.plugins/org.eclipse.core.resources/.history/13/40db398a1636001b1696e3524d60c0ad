package com.koreait.first.obj4;

public class PolyTest3 {

	public static void main(String[] args) {
		Dog dog = new Dog();
		Cat cat = new Cat();
		Bird bird = new Bird();
		
		polyCry(dog);
		polyCry(cat);
		polyCry(bird);
	}
	
	public static void polyCry(Animal animal) {
		animal.crying();
		
		if(animal instanceof Dog) {
			Animal animal2 = (Dog)animal2;
			//((Dog) animal).shakeTail();
			animal2.shakeTail();
		}
	}
}
