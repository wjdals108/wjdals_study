package com.koreait.first.starcraft;

public class StarCraftTest {

	public static void main(String[] args) {
		AttackUnit m1 = new Marine();
		AttackUnit m2 = new Marine();
		AttackUnit f1 = new Firebat();
		Unit md = new Medic();
		
		m1.attack(m2);
		f1.attack(m2);
		m2.showMyHp();
		
		md.heal(m2);
		m2.showMyHp();
		
		
	}
}
