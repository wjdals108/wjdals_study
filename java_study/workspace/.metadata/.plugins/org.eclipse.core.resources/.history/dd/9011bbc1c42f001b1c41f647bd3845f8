package com.koreait.first.loop;

public class For7 {

	public static void main(String[] args) {
		int star = 5;
		int count = star;
		
		
		for(int i=0; i<star; i++) {
			if(count>0) {
				System.out.println("_");
			}
			else
				for(int j=0; j<i+1; j++) {
					System.out.print("*");
					count--;
				}
			
			System.out.println();
			
		}		
	}
}